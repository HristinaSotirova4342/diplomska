    .wrapper {
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%,-50%);
        width: 950px;
        height: 650px;
        display: flex;
    }

        .wrapper .left {
            width: 50%;
            background-color: white;
            border-top-left-radius: 45px;
            border-bottom-left-radius: 45px;
            /*color: white;*/
