            /*background: linear-gradient(95deg, rgba(62,62,62,1) 2%, rgba(238,238,238,1) 25%, rgba(255,255,255,1) 29%);*/
        }

            .wrapper .left::before {
                content: '';
                width: 2px;
                height: 100%;
                background-color: rgba(255,0,0,0.6);
            }

        .wrapper .left {
            position: relative;
        }

            .wrapper .left::before {
                position: absolute;
                top: 0;
                right: 40px;
            }

